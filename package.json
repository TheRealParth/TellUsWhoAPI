{
  "name": "cooe-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "pm2 start ./bin/www --name cooe-api --watch",
    "dev": "ADDRESS=localhost PORT=8080 pm2 start ./bin/www --watch --name cooe-api",
    "server": "ADDRESS=resources.coo-e.com PORT=8080 pm2 start ./bin/www --name cooe-api -i 3",
    "nocluster": "ADDRESS=resources.coo-e.com PORT=8080 pm2 start ./bin/www --name cooe-api",
    "stop": "pm2 stop cooe-api",
    "restart": "pm2 restart ./bin/www --name cooe-api && pm2 logs cooe-api",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
